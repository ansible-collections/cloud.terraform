terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "=4.38.0"
    }
  }
}

provider "aws" {
}

variable "key_pair_name" {
  type = string
}

resource "aws_key_pair" "ansible-test" {
  key_name   = var.key_pair_name
  public_key = "ssh-rsa  email@example.com"
}
